{
  "name": "thinkerytim/oauth2-flexmls",
  "description": "FlexMLS OAuth 2.0 support for the PHP League's OAuth 2.0 Client",
  "license": "MIT",
  "authors": [
    {
      "name": "Tim Kramer",
      "email": "tim@thethinkery.net",
      "homepage": "https://www.thethinkery.net"
    }
  ],
  "keywords": [
    "php",
    "flexmls",
    "reso",
    "api",
    "oauth",
    "oauth2",
    "client",
    "authorization",
    "authentication",
    "spark platform"
  ],
  "require": {
    "php": ">=7.2",
    "league/oauth2-client": "^2.0"
  },
  "require-dev": {
    "phpunit/phpunit": "^9.2",
    "mockery/mockery": "^1.4"
  },
  "autoload": {
    "psr-4": {
      "ThinkeryTim\\OAuth2\\Client\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "ThinkeryTim\\OAuth2\\Client\\Test\\": "tests/src/"
    }
  },
  "scripts": {
    "test": "vendor/bin/phpunit"
  }
}
